<?xml version="1.0" encoding="utf-8" ?>
<views:BaseContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                       xmlns:views="clr-namespace:MauiApp1.Views"
                       x:Class="MauiApp1.Views.Page2"
                       Title="Page 2">
    <Grid ColumnDefinitions="*"
          RowDefinitions="*,Auto">

        <Label Grid.Row="0" Text="Page 2" />

        <Grid Grid.Row="1"
              ColumnDefinitions="*,*"
              RowDefinitions="Auto">

            <Button Grid.Column="0" Text="Logoff" Clicked="Logoff_Clicked" />
            <Button Grid.Column="1" Text="Next" Clicked="Next_Clicked" />
        </Grid>
    </Grid>
</views:BaseContentPage>